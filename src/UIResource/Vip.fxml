<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.VipController">
   <children>
      <Label layoutX="23.0" layoutY="25.0" text="贵宾管理系统">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <TableView fx:id="table" layoutX="114.0" layoutY="70.0" prefHeight="494.0" prefWidth="669.0">
        <columns>
          <TableColumn fx:id="col_name" prefWidth="75.0" text="姓名" />
          <TableColumn fx:id="col_sex" prefWidth="75.0" text="性别" />
            <TableColumn fx:id="col_tel" prefWidth="78.0" text="联系电话" />
            <TableColumn fx:id="col_address" prefWidth="224.0" text="联系地址" />
            <TableColumn fx:id="col_degree" minWidth="0.0" prefWidth="93.0" text="会员等级" />
            <TableColumn fx:id="col_time" maxWidth="300.0" prefWidth="128.0" text="注册时间" />
        </columns>
      </TableView>
      <Button layoutX="29.0" layoutY="83.0" mnemonicParsing="false" opacity="0.06" prefHeight="30.0" prefWidth="104.0" text="Button" />
      <Button fx:id="btn_showall" layoutX="23.0" layoutY="83.0" mnemonicParsing="false" onAction="#showall" text="显示全部" />
      <Button fx:id="btn_search" layoutX="23.0" layoutY="128.0" mnemonicParsing="false" onAction="#search_info" text="查找贵宾" />
      <Button fx:id="btn_add" layoutX="23.0" layoutY="172.0" mnemonicParsing="false" onAction="#add_info" text="添加贵宾" />
      <Button fx:id="btn_delete" layoutX="23.0" layoutY="220.0" mnemonicParsing="false" onAction="#del_info" text="删除贵宾" />
   </children>
</AnchorPane>
