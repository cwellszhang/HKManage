            <TableColumn fx:id="col_priority" prefWidth="98.0" text="权限" />
            <TableColumn fx:id="col_salary" prefWidth="75.0" text="薪资" />
            <TableColumn fx:id="col_account" prefWidth="75.0" text="账号" />
            <TableColumn fx:id="col_password" prefWidth="115.0" text="密码" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
