            <TableColumn fx:id="col_id" prefWidth="52.0" text="id" visible="false" />
            <TableColumn fx:id="col_name" prefWidth="98.0" text="姓名" />
            <TableColumn fx:id="col_sex" prefWidth="75.0" text="性别" />
